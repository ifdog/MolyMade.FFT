<Window x:Class="MolyMade.FFT.ImageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MolyMade.FFT"
        mc:Ignorable="d"
        Title="ImageWindow" Height="240" Width="320" SizeToContent="WidthAndHeight" MinWidth="320" MinHeight="240">
    <Grid>
        <Image x:Name="image" Margin="0,0,0,22"/>
        <Button x:Name="button" Content="Save As" Margin="0,166,10,0" Height="25" VerticalAlignment="Top" HorizontalAlignment="Right" Width="70" Click="button_Click"/>

    </Grid>
</Window>
